prevent.input();status.line.off();text.screen();lastChar = 0;reset(haveMatch);display(2, 1, "            KING'S QUEST II");display(3, 1, "                 Help");if ((machineType == ATARI ||     machineType == AMIGA))
  {  display(6, 1, "    F1 or HELP Displays this message.");  }else
  {  display(6, 1, "    F1 Displays this message.");  }display(7, 1, "    F2 Turns the sound off and on.");display(8, 1, "    F3 Retypes the last line typed.");display(9, 1, "    F5 Saves your current game.");display(10, 1, "    F7 Restores a saved game.");display(11, 1, "    F9 Restarts the game.");display(14, 1, "     = Makes King Graham swim.");if ((machineType == ATARI ||     machineType == AMIGA))
  {  display(15, 1, "   TAB Shows status screen. (see below)");  }else
  {  display(15, 1, "   TAB Shows the status screen.");  }display(16, 1, "   ESC Pauses the game.");display(17, 1, "Ctrl-J Sets up your joystick.");if (machineType == PC)
  {  display(18, 1, "Ctrl-R Toggles RGB monitor modes.");  }if ((machineType == ATARI ||     machineType == AMIGA))
  {  display(18, 1, "Double clicking joystick button or\n right mouse button also "          "shows status\n screen.");  }if (machineType == TANDY)
  {  display(21, 1, "    +  Increases volume.");  display(22, 1, "    -  Decreases volume.");  }Label1:if (!have.key())
  {  goto(Label1);  }accept.input();status.line.on();graphics();set(haveMatch);return();[ Messages#message 1 "            KING'S QUEST II"#message 2 "                 Help"#message 3 "    F1 Displays this message."#message 4 "    F2 Turns the sound off and on."#message 5 "    F3 Retypes the last line typed."#message 7 "    F5 Saves your current game."#message 9 "    F7 Restores a saved game."#message 11 "    F9 Restarts the game."#message 13 "   TAB Shows the status screen."#message 14 "   ESC Pauses the game."#message 15 "Ctrl-J Sets up your joystick."#message 16 "Ctrl-R Toggles RGB monitor modes."#message 17 "    F1 or HELP Displays this message."#message 18 "   TAB Shows status screen. (see below)"#message 19 "Double clicking joystick button or\n right mouse button also shows status\n screen."#message 20 "     = Makes King Graham swim."#message 22 "    +  Increases volume."#message 23 "    -  Decreases volume."