if (said("check out", "flag"))  {  print("drown.swim = %v95\ngenie.delay = %v88");  if (in.boat)    {    print("in.boat is set");    }  if (onWater)    {    print("on.water is set");    }  if (keep.cycling)    {    print("keep.cycling is set");    }  if (ego.flying)    {    print("ego.flying is set");    }  if (snakeInAction)    {    print("snakeInAction is set");    }  if (hag.in.room)    {    print("hag.in.room is set");    }  if (enchanter.in.room)    {    print("enchanter.in.room is set");    }  if (dwarf.in.room)    {    print("dwarf.in.room is set");    }  if (is.carpet.room)    {    print("is.carpet.room is set");    }  if (egoFallingCliff)    {    print("egoFallingCliff is set");    }  if (ego.falling.chasm)    {    print("ego.falling.chasm is set");    }  if (ego.fell)    {    print("ego.fell is set");    }  }if (said("tp"))  {  get.num("new room: ", debug.0);  new.room.v(debug.0);  }if ((said("position") ||     said("pos")))  {  get.num("x: ", debug.0);  get.num("y: ", debug.1);  erase(ego);  position.v(ego, debug.0, debug.1);  draw(ego);  }if (said("acquire", "object"))  {  get.num("object number: ", debug.0);  get.v(debug.0);  }if (said("drop", "var"))  {  get.num("var number: ", debug.0);  get.num("var value: ", debug.1);  *debug.0 = debug.1;  }if ((said("object") ||     said("object")))  {  get.num("object #: ", debug.0);  obj.status.v(debug.0);  }if (controller(c.show.priority))  {  show.pri.screen();  }if ((said("show", "var") ||     said("sv")))  {  get.num("var number: ", v111);  v126 = *v111;  print("var %v111: %v126");  }if ((said("show", "flag") ||     said("sf")))  {  get.num("flag number: ", v111);  if (issetv(v111))    {    print("flag %v111 is set");    }  else    {    print("flag %v111 is not set");    }  }if (said("reset", "flag"))  {  get.num("flag number: ", v111);  reset.v(v111);  }if (said("drop", "flag"))  {  get.num("flag number: ", v111);  set.v(v111);  }if (controller(c.show.mem))  {  show.mem();  }if (logging)  {  if (unknownWordNum == 1)    {    log("%m12%w1");    }  if (unknownWordNum == 2)    {    log("%m12%w2");    }  if (unknownWordNum == 3)    {    log("%m12%w3");    }  if (unknownWordNum == 0 &&       haveInput && !haveMatch)    {    log("No response to input.");    }  }return();[ Messages#message 1 "new room: "#message 2 "x: "#message 3 "y: "#message 4 "object number: "#message 5 "var number: "#message 6 "var value: "#message 7 "object #: "#message 8 "var %v111: %v126"#message 9 "flag number: "#message 10 "flag %v111 is set"#message 11 "flag %v111 is not set"#message 12 "Word not known: "#message 13 "%m12%w1"#message 14 "%m12%w2"#message 15 "%m12%w3"#message 16 "No response to input."#message 20 "drown.swim = %v95\ngenie.delay = %v88"#message 21 "in.boat is set"#message 22 "on.water is set"#message 23 "keep.cycling is set"#message 24 "ego.flying is set"#message 25 "snakeInAction is set"#message 26 "hag.in.room is set"#message 27 "enchanter.in.room is set"#message 28 "dwarf.in.room is set"#message 30 "is.carpet.room is set"#message 31 "egoFallingCliff is set"#message 32 "ego.falling.chasm is set"#message 33 "ego.fell is set"