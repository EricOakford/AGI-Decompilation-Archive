if (newRoom)  {  set(is.water.room);  reset(start.swimming);  return();  }if ((isset(handsOff) ||     distance.from.shore > 2 ||     drown.timer > 0))  {  goto(Label1);  }if (!start.swimming)  {  if ((said("swim", "anyword") ||       said("enter", "swim") ||       said("swim") ||       controller(c.swim)))    {    set(start.swimming);    if (!onWater)      {      print("You need to be in the water in order to swim.");      }    else      {      if (onWater &&           drown.swim == 2)        {        print("You are already swimming!");        }      else        {        reset(haveMatch);        }      }    }  }else  {  if (!controller(c.swim))    {    reset(start.swimming);    }  }if (onWater)  {  if (drown.swim <= 0)    {    set(keep.cycling);    splash.time = 250;    drown.swim = 1;    if (override.splash.loc)      {      reposition.to.v(ego, temp.x, temp.y);      }    else      {      object.on.water(ego);      }    get.posn(ego, temp.x, temp.y);    set.view(ego, View104);    work = 2;    cycle.time(ego, work);    stop.motion(ego);    if ((poisoned.water || turbulent.water))      {      drown.timer = 25;      }    }  else    {    if (!poisoned.water &&         (said("swim") ||         said("enter", "swim") ||         said("swim", "anyword") ||         controller(c.swim)))      {      set.view(ego, View97);      work = 1;      cycle.time(ego, work);      start.motion(ego);      get.posn(ego, temp.x, temp.y);      v82 = 0;      move.obj.v(ego, temp.x, temp.y, v82, done);      drown.swim = 2;      }    }  }if (drown.swim == 1 &&     drown.timer <= 0)  {  if (splash.time == 0)    {    set(normal.water);    drown.timer = 25;    }  else    {    --splash.time;    }  }if (!onWater &&     drown.swim == 2)  {  reset(keep.cycling);  drown.swim = 0;  set.view(ego, View0);  work = 1;  cycle.time(ego, work);  if (override.surface.loc)    {    reposition.to.v(ego, temp.x, temp.y);    }  else    {    object.on.land(ego);    }  }reset(override.splash.loc);reset(override.surface.loc);temp.x = 0;temp.y = 0;Label1:--drown.timer;if (drown.timer == 10)  {  if (poisoned.water)    {    print("This is a poisoned lake! Too bad, you die of extreme poisoning.");    }  else    {    if (turbulent.water)      {      print("The water is too turbulent to swim in. You have just drowned.");      }    else      {      if (normal.water)        {        print("You splash around in the water for a while but seem to have "              "forgotten how to swim. Unfortunately, you drown.");        }      }    }  }if (drown.timer == 1)  {  erase(ego);  set(certain.death);  }return();[ Messages#message 1 "You splash around in the water for a while but seem to have forgotten how to swim. Unfortunately, you drown."#message 2 "This is a poisoned lake! Too bad, you die of extreme poisoning."#message 3 "You need to be in the water in order to swim."#message 4 "You are already swimming!"#message 5 "The water is too turbulent to swim in. You have just drowned."