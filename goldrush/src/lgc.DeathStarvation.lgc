[***********************************************************[[ lgc.DeathStarvation[[ Displays the appropriate message and manages end of game[ when Jerrod dies from starvation.[[***********************************************************[ load and play the funeral dirgeload.sound(m.Death);sound(m.Death, DoneNoAction);[ after dying, no more player inputprevent.input();[ dead people don't movestop.motion(ego);[ set flag so game cycle will handle[ sequencing to get to restore/restartset(JerrodIsDead);[ clear all text lines below pictureclear.lines(22, 24, BLACK);[ message to display depends on exact manner of starvationif (EgoHealthStatus == 25)  {  display(22, 0, "This is one of the problems of no refrigeration!");  }if (EgoHealthStatus == 26)  {  display(22, 0, "There was something around to eat, you just needed to find "          "it!!");  }  return();[ **************************************[ MESSAGES[ **************************************#message 8 "This is one of the problems of no refrigeration!"#message 9 "There was something around to eat, you just needed to find it!!"