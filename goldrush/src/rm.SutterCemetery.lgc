[***********************************************************[[ rm.Sutter_Cemetery[ [ In the Sutters Fort cemetery, Jerrod needs to find the [ grave of Wilson Marshall to find an important clue from[ his brother Jake.[[***********************************************************[ **************************************[ FIRST CYCLE ONLY[ **************************************if (isset(newRoom))  {  [ load mule logic  load.logics(lgc.Mule);    [ load picture for current room  load.pic(currentRoom);    [ load view used in the room  load.view(vw.JerrodCA);    [ draw the current room picture  draw.pic(currentRoom);    [ Jerrod, with normal California attire  animate.obj(ego);  set.view(ego, vw.JerrodCA);    [ if returning from the closeup of Marshall's  [ tombstone   if (previousRoom == rm.MarshallGrave)    {    [ set loop and direction    set.loop(ego, 3);    egoDir = STOPPED;    [ position Jerrod in front of Marshall's    [ tombstone    position(ego, 83, 66);    set.string(inputPrompt, "%g69");    accept.input();    }  egoDir = STOPPED;  draw(ego);    [ if Jerrod has a mule  if ((has(LazyMule) || has("James' ol' mule")))    {    [ do mule initialization stuff    call(lgc.Mule);    }      [ now show the picture  show.pic();    [ not necessary to reset this flag; AGI resets it  [ automatically  reset(newRoom);    [ this flag is already reset when coming to this room, so  [ this line is unnecessary  reset(CycleAtRest);  }  [ **************************************[ EVERY CYCLE[ **************************************[ no additional commands [ **************************************[ CHECK FOR AN EXIT CONDITION[ **************************************[ going right or left leads to the area overview mapif ((posn(ego, 1, 43, 1, 166) ||     posn(ego, 1, 43, 152, 43) ||     posn(ego, 152, 43, 152, 166)))  {  new.room(rm.SuttersFortArea); [ ##LE001##  }  [ bottom goes back to the cemetery gate in the fortif (posn(ego, 0, 166, 159, 166))  {  [ position Jerrod at top-center in the gate room  position(ego, 26, 80);  new.room(rm.SF_CemeteryGate); [ ##LE002##  }  [ **************************************[ PROCESS PLAYER INPUT[ **************************************[ if player hasn't provided input no need to do said testsif (!isset(haveInput))  {  goto(Done);  }  [ look tombstone/look graveif ((said("examine", "gravestone") ||     said("examine", "rock") ||     said("examine", "grave")))  {  print("The stones here are roughly hewn; some are old, some are new.");  }  [ read tombstoneif ((said("read", "gravestone") ||     said("read", "grave") ||     said("read", "rock")))  {  [ if Jerrod is not next to one of the tombstones  if ((posn(ego, 1, 43, 158, 47) ||       posn(ego, 1, 48, 6, 58) ||       posn(ego, 20, 48, 48, 58) ||       posn(ego, 63, 48, 107, 58) ||       posn(ego, 122, 48, 134, 58) ||       posn(ego, 149, 48, 158, 58) ||       posn(ego, 9, 59, 31, 68) ||       posn(ego, 46, 59, 76, 68) ||       posn(ego, 93, 59, 147, 68) ||       posn(ego, 1, 69, 16, 82) ||       posn(ego, 37, 69, 57, 82) ||       posn(ego, 74, 69, 57, 82) ||       posn(ego, 132, 69, 158, 82) ||       posn(ego, 1, 83, 41, 94) ||       posn(ego, 60, 83, 147, 94) ||       posn(ego, 1, 95, 16, 111) ||       posn(ego, 34, 95, 69, 111) ||       posn(ego, 112, 95, 127, 111) ||       posn(ego, 146, 95, 158, 111) ||       posn(ego, 1, 112, 158, 167)))    {    print("These stones are hard to read unless yer spittin' close to 'em.");    }      [ if close to a tombstone, response depends on which one  [ Jerrod is close to  if (posn(ego, 1, 59, 8, 68))    {    print("\nUnknown\n 1844\n\n");    }  if (posn(ego, 7, 48, 19, 58))    {    print("\n      J. S. Jern\nBorn November 27, 1806\n Died August 31, "          "1848\n\n");    }  if (posn(ego, 17, 69, 36, 82))    {    print("\n   Katya M. Brosinske\n Departed this life on\n      May 19, "          "1846\nAged 53 years and 21 days\n\n");    }  if (posn(ego, 17, 95, 33, 111))    {    print("\n  Josiah Daly\n     1847\nDied in Infancy\n\n");    }  if (posn(ego, 32, 59, 45, 68))    {    print("\n     Here lies a man\n   whose crown was won\nby blowing in an "          "empty gun\n\n       William Able\n    died June 19,1847\n      aged "          "24 years\n\n");    }  if (posn(ego, 42, 83, 59, 94))    {    print("\nMarie Elizabeth Hanforth\n  and infant daughter\n      Died 1848\n "          "\n    God Knows Why\n\n");    }  if (posn(ego, 49, 48, 62, 58))    {    print("\n    Hezekiah Smith\n A Millwright by Trade\n       "          "1819-1845\n\nThis is what I expected,\n    But not so soon.\n\n");    }  if (posn(ego, 58, 69, 73, 82))    {    print("\nJose Sanchez\n Born 1792\n Died 1846\n\n");    }  if (posn(ego, 70, 95, 90, 111))    {    print("\n Dr. Samuel P. Upton\nLaid to rest July 1848\n     Aged 56 "          "years\n\n   Office Up Stairs\n\n");    }  if (posn(ego, 91, 95, 111, 111))    {    print("\n Thomas O'Flaherty\n   A fine friend\nand a loving father\n     "          "1800-1847\n\n");    }  if (posn(ego, 108, 48, 121, 58))    {    print("\n  Virginia E. Holden\nMoved to a better place\n      on May 1846\n "          "      At age 19\n\n");    }  if (posn(ego, 118, 69, 131, 82))    {    print("\n      R. L. Burdett\n        1791-1846\n\n He did acts of "          "kindness\nand charity as stealthily\nas some men commit crimes.\n\n");    }  if (posn(ego, 128, 95, 145, 111))    {    print("\n   Here lies\n\n Lester Moore \n  Four slugs\n  from a 44\n    No "          "Les   no  more\n\n");    }  if (posn(ego, 135, 48, 148, 58))    {    print("\n Little Arrow\nDied April 1848\n\n");    }  if (posn(ego, 148, 59, 158, 68))    {    print("\n   Prudence Blackman\n       1800-1847\n\nGone, but not "          "forgotten.\n\n");    }  if (posn(ego, 148, 83, 158, 94))    {    print("\n    Here lies an\n      atheist\n\n   All dressed up\n and no "          "place to go \n\n     1809-1848\n\n");    }      [ if reading Wilson Marshall's tombstone  if (posn(ego, 77, 59, 92, 68))    {    [ if not scored yet    if (!isset(ScoreMarshallGrave))      {      [ score two points      currentScore  += 2;      }    [ go to close up view of Marshall's tombstone    new.room(rm.MarshallGrave); [ ##LE003##    }  }  [ look fortif (said("examine", "fort"))  {  print("The adobe fort seems well-built.");  }  [ look hillsif (said("examine", "foot hill"))  {  print("Them mountains in the distance look grand.");  }  [ look groundif (said("examine", "dirt"))  {  print("The ground looks fertile.");  }  [ look grassif (said("examine", "grass"))  {  print("The grass is quite lush!");  }  [ look roadif (said("examine", "path"))  {  print("It's just a dirt road.");  }  [ look around/lookif ((said("examine", "around") ||     said("examine") ||     said("examine", "cemetery")))  {  print("Yer standin' in the cemetery of New Helvetia (Sutter's Fort). This "        "ain't a very lively place, but it may be interestin'!");  }  [ use shovel/dig for goldif ((said("use", "shovel") ||     said("dig", "gold") ||     said("dig") ||     said("dig", "hole")))  {  [ if Jerrod has a shovel  if (has("Shovel"))    {    print("Diggin' ain't allowed here! Do that somewhere else!!");    }  else    {    print("No shovel, no diggin'!");    }  }  [ use pan/pan for goldif ((said("use", "pan") ||     said("pan", "gold") ||     said("pan")))  {  print("Ya needs to be in the water!");  }  [ use letter/put letter on tombstoneif ((said("use", "letter") ||     said("use", "letter", "grave") ||     said("use", "letter", "rock") ||     said("use", "letter", "gravestone") ||     said("use", "hole") ||     said("use", "hole", "grave") ||     said("use", "hole", "rock") ||     said("use", "hole", "gravestone") ||     said("place", "letter") ||     said("place", "letter", "grave") ||     said("place", "letter", "rock") ||     said("place", "letter", "gravestone") ||     said("hold", "letter") ||     said("hold", "letter", "grave") ||     said("hold", "letter", "rock") ||     said("hold", "letter", "gravestone") ||     said("hold", "up") ||     said("find", "letter", "hole")))  {  [ if Jerrod has the letter from Jake  if (has("Letter"))    {    print("Incredible idea!!  But somethin' could be a little better!");    }  else    {    print("Ya ain't got a letter!");    }  }  Done:[ if Jerrod has a muleif ((has(LazyMule) || has("James' ol' mule")))  {  [ call mule logic to handle mule related commands  call(lgc.Mule);  }  return();[ **************************************[ MESSAGES[ **************************************#message 2 "The stones here are roughly hewn; some are old, some are new."#message 6 "These stones are hard to read unless yer spittin' close to 'em."#message 7 "\nUnknown\n 1844\n\n"#message 8 "\n      J. S. Jern\nBorn November 27, 1806\n Died August 31, 1848\n\n"#message 9 "\n   Katya M. Brosinske\n Departed this life on\n      May 19, 1846\nAged 53 years and 21 days\n\n"#message 10 "\n  Josiah Daly\n     1847\nDied in Infancy\n\n"#message 11 "\n     Here lies a man\n   whose crown was won\nby blowing in an empty gun\n\n       William Able\n    died June 19,1847\n      aged 24 years\n\n"#message 12 "\nMarie Elizabeth Hanforth\n  and infant daughter\n      Died 1848\n \n    God Knows Why\n\n"#message 13 "\n    Hezekiah Smith\n A Millwright by Trade\n       1819-1845\n\nThis is what I expected,\n    But not so soon.\n\n"#message 14 "\nJose Sanchez\n Born 1792\n Died 1846\n\n"#message 15 "\n Dr. Samuel P. Upton\nLaid to rest July 1848\n     Aged 56 years\n\n   Office Up Stairs\n\n"#message 16 "\n Thomas O'Flaherty\n   A fine friend\nand a loving father\n     1800-1847\n\n"#message 17 "\n  Virginia E. Holden\nMoved to a bette