oBlock3, 0);    set.cel(oBlock3, 7);    position(oBlock3, 135, 50);    set.priority(oBlock3, 5);    draw(oBlock3);    stop.cycling(oBlock3);    [ block 4    animate.obj(oBlock4);    set.view(oBlock4, vw.PanamaRouteArt);    set.loop(oBlock4, 0);    set.cel(oBlock4, 7);    position(oBlock4, 123, 59);    set.priority(oBlock4, 5);    draw(oBlock4);    stop.cycling(oBlock4);    [ block 5    animate.obj(oBlock5);    set.view(oBlock5, vw.PanamaRouteArt);    set.loop(oBlock5, 0);    set.cel(oBlock5, 7);    position(oBlock5, 117, 72);    set.priority(oBlock5, 5);    draw(oBlock5);    stop.cycling(oBlock5);    [ block 6    animate.obj(oBlock6);    set.view(oBlock6, vw.PanamaRouteArt);    set.loop(oBlock6, 0);    set.cel(oBlock6, 7);    position(oBlock6, 113, 85);    set.priority(oBlock6, 5);    draw(oBlock6);    stop.cycling(oBlock6);    [ block 7    animate.obj(oBlock7);    set.view(oBlock7, vw.PanamaRouteArt);    set.loop(oBlock7, 0);    set.cel(oBlock7, 7);    position(oBlock7, 102, 95);    set.priority(oBlock7, 5);    draw(oBlock7);    stop.cycling(oBlock7);        [ flashing marker - the marker is of a special size and    [ shape that uses the priority value of the route on the    [ picture to show the marker as a single flashing pixel    [ that moves along the trip path line    animate.obj(oMarker);    set.view(oMarker, vw.PanamaRouteArt);    set.loop(oMarker, 1);    fix.loop(oMarker);    position(oMarker, 134, 12);    set.priority(oMarker, 6);    ignore.horizon(oMarker);    ignore.objs(oMarker);    draw(oMarker);    vTmpVar = 2;    step.time(oMarker, vTmpVar);    [ move marker to first point    move.obj(oMarker, 135, 50, 1, fMarkerMoved);    vMovePoint = 1;    }      [ if trip timer is at 5th point  if (CurrentTripTime == TripPoint_5)    {    [ in this sequence, marker will move from Cuba to    [ Chagres, Panama        [ cover up the route line on the Pacific portion of the    [ trip    add.to.pic(vw.PanamaRouteArt, 0, 7, 114, 160, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 102, 164, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 92, 164, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 80, 164, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 69, 153, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 59, 140, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 50, 128, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 41, 119, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 31, 109, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 22, 100, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 18, 87, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 9, 78, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 8, 78, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 8, 65, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 8, 52, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 8, 39, 5, 5);    add.to.pic(vw.PanamaRouteArt, 0, 7, 8, 26, 5, 5);        [ use blocks to cover up the marker route; they will be    [ removed as the marker moves to show the traced route    animate.obj(oBlock1);    set.view(oBlock1, vw.PanamaRouteArt);    set.loop(oBlock1, 0);    set.cel(oBlock1, 7);    position(oBlock1, 103, 108);    set.priority(oBlock1, 5);    draw(oBlock1);    stop.cycling(oBlock1);    [ block 2    animate.obj(oBlock2);    set.view(oBlock2, vw.PanamaRouteArt);    set.loop(oBlock2, 0);    set.cel(oBlock2, 7);    position(oBlock2, 109, 121);    set.priority(oBlock2, 5);    draw(oBlock2);    stop.cycling(oBlock2);    [ block 3    animate.obj(oBlock3);    set.view(oBlock3, vw.PanamaRouteArt);    set.loop(oBlock3, 0);    set.cel(oBlock3, 7);    position(oBlock3, 109, 134);    set.priority(oBlock3, 5);    draw(oBlock3);    stop.cycling(oBlock3);    [ block 4    animate.obj(oBlock4);    set.view(oBlock4, vw.PanamaRouteArt);    set.loop(oBlock4, 0);    set.cel(oBlock4, 7);    position(oBlock4, 109, 147);    set.priority(oBlock4, 5);    draw(oBlock4);    stop.cycling(oBlock4);        [ flashing marker - the marker is of a special size and    [ shape that uses the priority value of the route on the    [ picture to show the marker as a single flashing pixel    [ that moves along the trip path line    animate.obj(oMarker);    set.view(oMarker, vw.PanamaRouteArt);    set.loop(oMarker, 1);    fix.loop(oMarker);    position(oMarker, 102, 107);    set.priority(oMarker, 6);    ignore.horizon(oMarker);    ignore.objs(oMarker);    draw(oMarker);    vTmpVar = 2;    step.time(oMarker, vTmpVar);    [ move marker to first point    move.obj(oMarker, 109, 146, 1, fMarkerMoved);    vMovePoint = 1;    }      [ if trip timer is at 19th point  if (CurrentTripTime == TripPoint_19)    {    [ in this sequence, marker will move from Panama City to    [ California        [ there are no sections of the trip path that need to be    [ permanently covered            [ use blocks to cover up the marker route; they will be    [ removed as the marker moves to show the traced route    animate.obj(oBlock1);    set.view(oBlock1, vw.PanamaRouteArt);    set.loop(oBlock1, 0);    set.cel(oBlock1, 7);    position(oBlock1, 114, 160);    set.priority(oBlock1, 5);    draw(oBlock1);    stop.cycling(oBlock1);    [ block 2    animate.obj(oBlock2);    set.view(oBlock2, vw.PanamaRouteArt);    set.loop(oBlock2, 0);    set.cel(oBlock2, 7);    position(oBlock2, 102, 164);    set.priority(oBlock2, 5);    draw(oBlock2);    stop.cycling(oBlock2);    [ block 3    animate.obj(oBlock3);    set.view(oBlock3, vw.PanamaRouteArt);    set.loop(oBlock3, 0);    set.cel(oBlock3, 7);    position(oBlock3, 92, 164);    set.priority(oBlock3, 5);    draw(oBlock3);    stop.cycling(oBlock3);    [ block 4    animate.obj(oBlock4);    set.view(oBlock4, vw.PanamaRouteArt);    set.loop(oBlock4, 0);    set.cel(oBlock4, 7);    position(oBlock4, 80, 164);    set.priority(oBlock4, 5);    draw(oBlock4);    stop.cycling(oBlock4);    [ block 5    animate.obj(oBlock5);    set.view(oBlock5, vw.PanamaRouteArt);    set.loop(oBlock5, 0);    set.cel(oBlock5, 7);    position(oBlock5, 69, 153);    set.priority(oBlock5, 5);    draw(oBlock5);    stop.cycling(oBlock5);    [ block 6    animate.obj(oBlock6);    set.view(oBlock6, vw.PanamaRouteArt);    set.loop(oBlock6, 0);    set.cel(oBlock6, 7);    position(oBlock6, 59, 140);    set.priority(oBlock6, 5);    draw(oBlock6);    stop.cycling(oBlock6);    [ block 7    animate.obj(oBlock7);    set.view(oBlock7, vw.PanamaRouteArt);    set.loop(oBlock7, 0);    set.cel(oBlock7, 7);    position(oBlock7, 50, 128);    set.priority(oBlock7, 5);    draw(oBlock7);    stop.cycling(oBlock7);    [ block 8    animate.obj(oBlock8);    set.view(oBlock8, vw.PanamaRouteArt);    set.loop(oBlock8, 0);    set.cel(oBlock8, 7);    position(oBlock8, 41, 119);    set.priority(oBlock8, 5);    draw(oBlock8);    stop.cycling(oBlock8);    [ block 9    animate.obj(oBlock9);    set.view(oBlock9, vw.PanamaRouteArt);    set.loop(oBlock9, 0);    set.cel(oBlock9, 7);    position(oBlock9, 31, 109);    set.priority(oBlock9, 5);    draw(oBlock9);    stop.cycling(oBlock9);    [ block 10    animate.obj(oBlock10);    set.view(oBlock10, vw.PanamaRouteArt);    set.loop(oBlock10, 0);    set.cel(oBlock10, 7);    position(oBlock10, 22, 100);    set.priority(oBlock10, 5);    draw(oBlock10);    stop.cycling(oBlock10);    [ block 11    animate.obj(oBlock11);    set.view(oBlock11, vw.PanamaRouteArt);    set.loop(oBlock11, 0);    set.cel(oBlock11, 7);    position(oBlock11, 18, 87);    set.priority(oBlock11, 5);    draw(oBlock11);    stop.cycling(oBlock11);    [ block 12    animate.obj(oBlock12);    set.view(oBlock12, vw.PanamaRouteArt);    set.loop(oBlock12, 0);    set.cel(oBlock12, 7);    position(oBlock12, 9, 78);    set.priority(oBlock12, 5);    draw(oBlock12);    stop.cycling(oBlock12);    [ block 13    animate.obj(oBlock13);    set.view(oBlock13, vw.PanamaRouteArt);    set.loop(oBlock13, 0);    set.cel(oBlock13, 7);    position(oBlock13, 8, 78);    set.priority(oBlock13, 5);    draw(oBlock13);    stop.cycling(oBlock13);    [ block 14    animate.obj(oBlock14);    set.view(oBlock14, vw.PanamaRouteArt);    set.loop(oBlock14, 0);    set.cel(oBlock14, 7);    position(oBlock14, 8, 65);    set.priority(oBlock14, 5);    draw(oBlock14);    stop.cycling(oBlock14);    [ block 15    animate.obj(oBlock15);    set.view(oBlock15, vw.PanamaRouteArt);    set.loop(oBlock15, 0);    set.cel(oBlock15, 7);    position(oBlock15, 8, 52);    set.priority(oBlock15, 5);    draw(oBlock15);    stop.cycling(oBlock15);    [ block 16    animate.obj(oBlock16);    set.view(oBlock16, vw.PanamaRouteArt);    set.loop(oBlock16, 0);    set.cel(oBlock16, 7);    position(oBlock16, 8, 39);    set.priority(oBlock16, 5);    ignore.horizon(oBlock16);    draw(oBlock16);    stop.cycling(oBlock16);    [ block 17    animate.obj(oBlock17);    set.view(oBlock17, vw.PanamaRouteArt);    set.loop(oBlock17, 0);    set.cel(oBlock17, 7);    position(oBlock17, 8, 26);    set.priority(oBlock17, 5);    ignore.horizon(oBlock17);    draw(oBlock17);    stop.cycling(oBlock17);        [ flashing marker - the marker is of a special size and    [ shape that uses the priority value of the route on the    [ picture to show the marker as a single flashing pixel    [ that moves along the trip path line    animate.obj(oMarker);    set.view(oMarker, vw.PanamaRouteArt);    set.loop(oMarker, 1);    fix.loop(oMarker);    position(oMarker, 114, 151);    set.priority(oMarker, 6);    ignore.horizon(oMarker);    ignore.objs(oMarker);    draw(oMarker);    vTmpVar = 2;    step.time(oMarker, vTmpVar);    [ move marker to first point    move.obj(oMarker, 114, 160, 1, fMarkerMoved);    vMovePoint = 1;    }      [ now show the picture  show.pic();  }  [ **************************************[ EVERY CYCLE[ **************************************[ check status of marker; as it reaches each point, move to[ next point, and remove the blocks as the cursor moves onto[ them[ if current trip segment is New York to Cubaif (CurrentTripTime == TripPoint_3)  {  [ each time marker reaches its end point move it to next  [ point  if (isset(fMarkerMoved) && vMovePoint == 1)    {    reset(fMarkerMoved);    vMovePoint = 2;    move.obj(oMarker, 125, 57, 1, fMarkerMoved);    }  if (isset(fMarkerMoved) && vMovePoint == 2)    {    reset(fMarkerMoved);    vMovePoint = 3;    move.obj(oMarker, 117, 81, 1, fMarkerMoved);    }  if (isset(fMarkerMoved) && vMovePoint == 3)    {    reset(fMarkerMoved);    vMovePoint = 4;    move.obj(oMarker, 104, 85, 1, fMarkerMoved);    }  if (isset(fMarkerMoved) && vMovePoint == 4)    {    reset(fMarkerMoved);    vMovePoint = 5;    move.obj(oMarker, 102, 95, 1, fMarkerMoved);    }  if (isset(fMarkerMoved) && vMovePoint == 5)    {    reset(fMarkerMoved);    vMovePoint = 6;    move.obj(oMarker, 102, 107, 1, fMarkerMoved);    }  [ when marker reaches last point  if (isset(fMarkerMoved) && vMovePoint == 6)    {    reset(fMarkerMoved);    vMovePoint = 7;    [ increment the counter here, so time mark 4 is skipped    [ (time point 4 is not used)    ++CurrentTripTime;    [ continue the journey    set(ContinueJourney);    }      [ use marker Y position to determine when to remove the  [ blocks  get.posn(oMarker, vMarkerX, vMarkerY);  if (vMarkerY == 24)    {    erase(oBlock1);    }  if (vMarkerY == 37)    {    erase(oBlock2);    }  if (vMarkerY == 50)    {    erase(oBlock3);    }  if (vMarkerY == 59)    {    erase(oBlock4);    }  if (vMarkerY == 72)    {    erase(oBlock5);    }  if (vMarkerY == 85)    {    erase(oBlock6);    }  if (vMarkerY == 95)    {    erase(oBlock7);    }      [ at specific points along the way, show informational  [ messages about the trip, unless player has turned off  [ **************************************[ MESSAGES[ **************************************  if (posn(oMarker, 135, 40, 135, 40) && vMsgCounter == 0)    {    ++vMsgCounter;    if (!isset(HideTripMsgs))      {      print("The Panama route, since it is in most cases the most expensive "            "route, generally attracts the slightly more sophisticated "            "passenger. But the jungle has no respect for sophistication, only "            "surprises! Some of these people haven't seen a hard day's work in "            "their entire lives, save chopping that firewood for mother.");      print("But nonetheless, it has become the route taken by New York "            "brokers, Philadelphia lawyers, Boston bankers, and big city "            "doctors.");      print("The captain advises the passengers that it will take approximately "            "two to three weeks to get to the Rio Chagres.");      }    }  if (posn(oMarker, 120, 62, 120, 62) && vMsgCounter == 1)    {    ++vMsgCounter;    if (!isset(HideTripMsgs))      {      print("There is one main advantage of this route over the Cape Horn "            "route. It normally takes less time than the Cape Horn route, "            "getting you to that California Gold as soon as possible!!");      print("That is if everything goes right! You are taking a big chance on "            "catching a ride on the Pacific side of the isthmus. You must also "            "contend  with the Panamanian jungle.");      }    }      [ at specific points along the trip, increment the game  [ time by the number of days on the trip  if (posn(oMarker, 135, 23, 135, 23) && vTimeCounter == 0)    {    vTimeCounter = 1;    GameDays  += 3;    }  if (posn(oMarker, 135, 50, 135, 50) && vTimeCounter == 1)    {    vTimeCounter = 2;    GameDays  += 3;    }  if (posn(oMarker, 119, 63, 119, 63) && vTimeCounter == 2)    {    vTimeCounter = 3;    GameDays  += 3;    }  if (posn(oMarker, 110, 85, 110, 85) && vTimeCounte