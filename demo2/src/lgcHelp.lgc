status.line.off();text.screen();lastChar = 0;reset(haveMatch);display(5, 1, "   Space Quest\\\\King's Quest ]I[ Demo");display(6, 1, "                Help");if ((machineType == ATARI ||     machineType == AMIGA))  {  display(9, 1, "    F1 or HELP displays this message.");  }else  {  display(9, 1, "    F1 Displays this message.");  }if (soundOn)  {  display(10, 1, "    F2 Turns the sound off.");  }else  {  display(10, 1, "    F2 Turns the sound on.");  }display(11, 1, "    F9 Restarts the demo.");display(12, 1, " Alt Z Exits demo.");display(14, 1, "   ESC pauses the demo.");if (machineType == PC)  {  display(15, 1, "Ctrl-R toggles RGB monitor modes.");  }if (machineType == TANDY)  {  display(15, 1, "    +  Increases volume.");  display(16, 1, "    -  Decreases volume.");  }display(19, 1, " To install on hard disk:");display(20, 1, "  type A:INSTALLH at dos prompt.");display(22, 1, " This disk is copyable with DISKCOPY.");Label1:if (!have.key())  {  goto(Label1);  }graphics();set(haveMatch);return();[ Messages#message 1 "   Space Quest\\\\King's Quest ]I[ Demo"#message 2 "                Help"#message 3 "    F1 Displays this message."#message 4 "    F2 Turns the sound off."#message 11 "    F9 Restarts the demo."#message 14 "   ESC pauses the demo."#message 16 "Ctrl-R toggles RGB monitor modes."#message 17 "    F1 or HELP displays this message."#message 20 "    +  Increases volume."#message 21 "    -  Decreases volume."#message 22 " To install on hard disk:"#message 23 "  type A:INSTALLH at dos prompt."#message 24 " This disk is copyable with DISKCOPY."#message 25 "    F2 Turns the sound on."#message 26 " Alt Z Exits demo."