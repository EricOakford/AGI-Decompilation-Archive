#define a.bowl  o1#define survivable.fall f20#define stun.done   f21#define deadly.fall f22#define beanstalk.sound.done    f25#define stunned.time v30if (newRoom)  {  load.pic(currentRoom);  draw.pic(currentRoom);  discard.pic(currentRoom);  load.sound(Sound4);  load.sound(s.ego.fall);  set.horizon(50);  if (goat.following)    {    load.logics(lgc.goat);    }  if (dead.goat.room == currentRoom)    {    add.to.pic(View62, 0, 0, 39, 96, 0, 0);    }  if ((have.beans ||       beanstalk.room == 31))    {    beanstalk.overlay = 84;    set(can.plant.beanstalk);    load.pic(beanstalk.overlay);    load.view(View114);    load.view(View103);    load.view(View104);    load.view(View74);    load.view(View105);    load.logics(lgc.beanstalk);    load.sound(Sound12);    }  if (previousRoom == 70 &&       climbing.beanstalk)    {    set.view(ego, View74);    position(ego, 64, 51);    move.obj(ego, 64, 51, 0, done);    set.priority(ego, 8);    }  if (previousRoom == 70 &&       falling.from.sky)    {    program.control();    ignore.horizon(ego);    set.priority(ego, 15);    set.view(ego, View105);    get.posn(ego, temp.x, temp.y);    if (temp.x > 75)      {      position(ego, 80, 29);      }    else      {      position(ego, 65, 29);      }    get.posn(ego, temp.x, temp.y);    temp.y = 106;    temp.step = 2;    move.obj.v(ego, temp.x, temp.y, temp.step, deadly.fall);    }  if (!took.bowl)    {    load.view(View25);    animate.obj(a.bowl);    set.view(a.bowl, View25);    position(a.bowl, 126, 138);    }  if (!isset(took.bowl))    {    draw(a.bowl);    }  if (beanstalk.room == 31)    {    overlay.pic(beanstalk.overlay);    }  draw(ego);  show.pic();  }if (falling.from.sky &&     !isset(lf3))  {  set(lf3);  set(ego.stunned);  sound(s.ego.fall, lf4);  }if (deadly.fall)  {  stop.sound();  set(lf3);  reset(falling.from.sky);  reset(ego.stunned);  set.view(ego, View114);  shake.screen(2);  set(certain.death);  }if (took.bowl && !have.bowl)  {  if (said("anyword", "bowl"))    {    print("There may have been a bowl here at one time, but it is not here now!");    }  }if (!took.bowl)  {  if (said("check", "bowl"))    {    if (posn(ego, 106, 103, 159, 167))      {      print("This is a very colorful ceramic bowl.");      }    else      {      print("It looks like an interesting, colorful bowl.");      }    }  if ((said("check", "grass") ||       said("check", "room")))    {    print("There appears to be some sort of bowl on the ground.");    }  }if (!took.bowl &&     said("acquire", "bowl"))  {  if (posn(ego, 116, 128, 136, 148))    {    print("You pick up the ceramic bowl.");    start.update(a.bowl);    erase(a.bowl);    currentScore  += 3;    set(have.bowl);    set(took.bowl);    get("ceramic bowl");    }  else    {    print("That is impossible from here.");    }  }if (have.beans && can.plant.beanstalk)  {  if (said("bury", "bean"))    {    reset(have.beans);    beanstalk.room = 31;    set(planted.beans);    drop("beans");    currentScore  += 2;    print("The beans immediately sprout roots that reach deep into the fertile "          "soil... leaves unfold themselves to the warm, loving sun... a "          "rumbling is felt... and a mighty beanstalk now stretches up into the "          "sky!");    sound(Sound12, beanstalk.sound.done);    overlay.pic(beanstalk.overlay);    show.pic();    }  }if (beanstalk.room == 31 &&     !ego.invisible)  {  if (said("climb", "beanstalk") &&       !climbing.beanstalk)    {    if (posn(ego, 52, 76, 103, 97))      {      erase(ego);      set.view(ego, View74);      set(climbing.beanstalk);      position(ego, 71, 77);      set.priority(ego, 8);      draw(ego);      move.obj(ego, 71, 77, 0, done);      }    else      {      print("You can't reach the beanstalk from here.");      }    }  }if (beanstalk.room != 31)  {  if (said("climb", "beanstalk"))    {    print("There is no beanstalk here to climb.");    }  }if (climbing.beanstalk)  {  if (egoHitSpecial &&       posn(ego, 60, 78, 93, 80))    {    erase(ego);    set.view(ego, View0);    reset(climbing.beanstalk);    position(ego, 70, 106);    release.priority(ego);    draw(ego);    }  if (egoHitSpecial &&       !posn(ego, 60, 78, 93, 80))    {    program.control();    set.priority(ego, 15);    get.posn(ego, temp.x, temp.y);    erase(ego);    set.view(ego, View105);    reset(climbing.beanstalk);    position.v(ego, temp.x, temp.y);    draw(ego);    temp.y = 106;    temp.step = 2;    move.obj.v(ego, temp.x, temp.y, temp.step, survivable.fall);    set(ego.stunned);    sound(s.ego.fall, fall.sound.done);    }  }--stunned.time;if (survivable.fall)  {  stop.sound();  reset(survivable.fall);  observe.blocks(ego);  release.priority(ego);  stop.motion(ego);  set.view(ego, View103);  stunned.time = 20;  set(keep.cycling);  shake.screen(2);  print("Ouch!");  set(ego.stunned);  sound(Sound4, sound.done);  }if (sound.done)  {  if (stunned.time > 0)    {    reset(sound.done);    sound(Sound4, sound.done);    }  }if (stunned.time == 1)  {  set.view(ego, View104);  end.of.loop(ego, stun.done);  }if (stun.done)  {  reset(stun.done);  reset(keep.cycling);  start.motion(ego);  set.view(ego, View0);  release.priority(ego);  player.control();  }if (edgeEgoHit != NOT_HIT)  {  reset(can.plant.beanstalk);  }if (edgeEgoHit == TOP_EDGE &&     climbing.beanstalk)  {  new.room(Logic70);  }if (edgeEgoHit == TOP_EDGE &&     !isset(climbing.beanstalk))  {  new.room(Logic34);  }if (edgeEgoHit == RIGHT_EDGE)  {  new.room(Logic32);  }if (edgeEgoHit == LEFT_EDGE)  {  new.room(Logic30);  }if (edgeEgoHit == BOTTOM_EDGE)  {  new.room(Logic18);  }if (goat.following)  {  call(lgc.goat);  }if (beanstalk.room == 31)  {  call(lgc.beanstalk);  }return();[ Messages#message 1 "This is a very colorful ceramic bowl."#message 2 "You pick up the ceramic bowl."#message 3 "It looks like an interesting, colorful bowl."#message 4 "That is impossible from here."#message 5 "There may have been a bowl here at one time, but it is not here now!"#message 6 "The beans immediately sprout roots that reach deep into the fertile soil... leaves unfold themselves to the warm, loving sun... a rumbling is felt... and a mighty beanstalk now stretches up into the sky!"#message 7 "You can't reach the beanstalk from here."#message 8 "There is no beanstalk here to climb."#message 9 "Ouch!"#message 10 "There appears to be some sort of bowl on the ground."