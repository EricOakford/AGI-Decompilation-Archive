#define drown.msg   f20if (newRoom)  {  load.pic(currentRoom);  draw.pic(currentRoom);  discard.pic(currentRoom);  set.horizon(46);  if (goat.following)    {    load.logics(lgc.goat);    }  if (dead.goat.room == currentRoom)    {    add.to.pic(View62, 0, 0, 110, 110, 0, 0);    }  load.logics(lgc.swim);  call(lgc.swim);  set(deadly.water);  if (previousRoom == 23)    {    if (onWater)      {      set.view(ego, View0);      reset(keep.cycling);      drown.swim = 0;      }    }  ignore.blocks(ego);  draw(ego);  show.pic();  }if ((said("check", "doorway") ||     said("look in", "doorway") ||     said("look in", "stump")))  {  if (posn(ego, 66, 134, 159, 167))    {    print("It is just a hole in an empty, old stump.");    }  else    {    print("You are not close enough.");    }  }if (said("check", "stump"))  {  print("It is old and rotted.");  }if (said("check", "river"))  {  print("The stream appears placid enough, but appearances can be deceiving.");  }if (said("check", "anyword"))  {  print("There is an attractive meadow dotted with trees.");  }if (have.bucket && !have.water)  {  if ((said("acquire", "water") ||       said("drink", "water")))    {    if (posn(ego, 0, 27, 95, 95))      {      print("The bank is too steep to allow access to the river's edge.");      }    else      {      print("You are not close enough to the river.");      }    }  }if (onWater && !drown.msg)  {  set(drown.msg);  print("This placid looking stream is actually quite dangerous.... the current "        "is stronger than it looks.  You can't keep your head above water.");  }if (edgeEgoHit != NOT_HIT)  {  reset(deadly.water);  }if (edgeEgoHit == TOP_EDGE)  {  new.room(Logic39);  }if (edgeEgoHit == RIGHT_EDGE)  {  new.room(Logic27);  }if (edgeEgoHit == LEFT_EDGE)  {  new.room(Logic25);  }if (edgeEgoHit == BOTTOM_EDGE)  {  new.room(Logic23);  }if (goat.following)  {  call(lgc.goat);  }call(lgc.swim);return();[ Messages#message 1 "This placid looking stream is actually quite dangerous.... the current is stronger than it looks.  You can't keep your head above water."#message 2 "The bank is too steep to allow access to the river's edge."#message 3 "You are not close enough to the river."#message 4 "The stream appears placid enough, but appearances can be deceiving."#message 5 "It is old and rotted."#message 6 "There is an attractive meadow dotted with trees."#message 7 "It is just a hole in an empty, old stump."#message 8 "You are not close enough."