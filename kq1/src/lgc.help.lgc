prevent.input();status.line.off();text.screen();lastChar = 0;reset(haveMatch);display(1, 1, "              KING'S QUEST");display(2, 1, "                 Help");if (machineType == ATARI)
  {  display(4, 1, "    F1 or HELP displays this message.");  display(11, 1, "   ESC activates the menus. (see below)");  display(14, 1, "     = for swimming (see below)");  display(15, 1, "Ctrl-J toggles joystick/mouse.");  display(17, 1, "Clicking the joystick button or the\n right mouse button also "          "activates\n the menus.");  display(21, 1, "Double-clicking the joystick button\n or the right mouse "          "button also\n initiates swimming.");  }else
  {  display(4, 1, "    F1 displays this message.");  display(11, 1, "   ESC activates the menus.");  display(14, 1, "     = for swimming");  display(15, 1, "Ctrl-J sets up your joystick.");  }display(5, 1, "    F2 turns the sound off and on.");display(6, 1, "    F3 retypes the last line typed.");display(7, 1, "    F5 saves your current game.");display(8, 1, "    F7 restores a saved game.");display(9, 1, "    F9 restarts the game.");display(10, 1, "   TAB shows the status screen.");display(12, 1, "     0 for jumping");display(13, 1, "     - for ducking");if (machineType == PC &&     monitorType != EGA &&     monitorType != MONO)
  {  display(16, 1, "Ctrl-R toggles graphics mode.");  }Label1:if (!have.key())
  {  goto(Label1);  }accept.input();status.line.on();graphics();set(haveMatch);return();[ Messages#message 1 "              KING'S QUEST"#message 2 "                 Help"#message 3 "    F1 displays this message."#message 4 "    F2 turns the sound off and on."#message 5 "    F3 retypes the last line typed."#message 6 "    F5 saves your current game."#message 7 "    F7 restores a saved game."#message 8 "    F9 restarts the game."#message 9 "   TAB shows the status screen."#message 10 "   ESC activates the menus."#message 11 "     0 for jumping"#message 12 "     - for ducking"#message 13 "     = for swimming"#message 14 "Ctrl-J sets up your joystick."#message 15 "Ctrl-R toggles graphics mode."#message 16 "    F1 or HELP displays this message."#message 17 "   ESC activates the menus. (see below)"#message 18 "     = for swimming (see below)"#message 19 "Ctrl-J toggles joystick/mouse."#message 20 "Clicking the joystick button or the\n right mouse button also activates\n the menus."#message 21 "Double-clicking the joystick button\n or the right mouse button also\n initiates swimming."